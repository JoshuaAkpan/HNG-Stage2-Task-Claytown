{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.timbu.cloud/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
